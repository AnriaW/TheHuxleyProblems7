#include <stdlib.h>
#include <stdbool.h>
#include <math.h>
#include <string.h>

int main(){
    int x, y, z;
    scanf("%d %d %d", &x, &y, &z);
    int i = 0;
    int j = 1;
    
    while (j <= y){

        i++;

        if ( i % x == 0){
            printf("%d\n", j);
        }
        else{
            printf("%d ", j);
        }
        
        j+= z;
    }
}
